            {% if question == "Sub_question_choice" or question == "Sub_question_free" or question == "Sub_question_scale" %}<!--niveau 3-->
            
                {# si la question est une sous question on ne l affiche pas (au cas ou la requete de tri ne fonctionne pas) #}

            {% else %}<!--niveau 3-->
            
                {% set questionnumber =  questionnumber + 1 %}<!--niveau 4-->

                {% if (question == "Conditional_question_free" or question == "Conditional_question_master" or question == "Conditional_question_choice")%}<!--niveau 4-->
                
