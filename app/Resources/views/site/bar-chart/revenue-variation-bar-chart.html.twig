<ul class="chart">
    <div id="monthlyExcedent" class="display flex-row-reverse justify-content-end" style="height: 100%;">
        {% for excedent in excedentMonth %}
            <li class="position-realtive" style="height:45%;">
                <div class="month">{{ litteralMonths[date] }}</div>
                <div class="full-height-bar gray">
                    <div class="percentage">{{ excedent }}%</div>
                </div>
                <div class="bar green" style="height: {{ excedent }}%;"></div>
            </li>
            {% if date > 1 %}
                {% set date = date - 1 %}
            {% else %}
                {% set date = 12 %}
            {% endif %}
        {% endfor %}
    </div>
    <div id="annualExcedent" class="hidden flex-row-reverse justify-content-end" style="height: 100%">
        {% set n = 0 %}
        {% for excedent in excedentYear %}
            <li class="position-realtive" style="height:45%;">
                <div class="month">{{ year[n] }}</div>
                <div class="full-height-bar gray">
                    <div class="percentage">{{ excedent }}%</div>
                </div>
                <div class="bar green" style="height: {{ excedent }}%;"></div>
            </li>
            {% set n =n+1 %}
        {% endfor %}
    </div>
</ul>